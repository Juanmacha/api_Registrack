  "query_verificacion_bd": "SELECT id_orden_servicio, tipodepersona, nombrecompleto, nombredelamarca, CASE WHEN poderparaelregistrodelamarca IS NOT NULL THEN 'OK' ELSE 'FALTA' END as poder_registro, CASE WHEN tipodepersona = 'Natural' AND poderdelrepresentanteautorizado IS NULL THEN 'OK' WHEN tipodepersona = 'Natural' AND poderdelrepresentanteautorizado IS NOT NULL THEN 'ERROR' ELSE 'OK' END as validacion_representante FROM ordenes_de_servicios ORDER BY id_orden_servicio DESC LIMIT 5;"
